{
  "name": "cms",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "copy:dev": "node run.cjs dev",
    "copy:prod": "node run.cjs prod",
    "start:dev": "npm run copy:dev && vite dev",
    "start:prod": "npm run copy:prod && vite dev",
    "build:dev": "npm run copy:dev && vite build",
    "build:prod": "npm run copy:prod && vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "1.40.1",
    "@sveltejs/adapter-static": "3.0.1",
    "@sveltejs/kit": "2.0.6",
    "@sveltejs/vite-plugin-svelte": "3.0.1",
    "@types/luxon": "3.3.7",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte": "2.35.1",
    "postcss": "8.4.32",
    "svelte": "4.2.8",
    "svelte-check": "3.6.2",
    "tailwindcss": "3.4.0",
    "tslib": "2.6.2",
    "typescript": "5.3.3",
    "vite": "5.0.10",
    "vitest": "1.1.0"
  },
  "type": "module",
  "dependencies": {
    "@jaspero/modular": "1.19.0",
    "@jaspero/utils": "1.15.3",
    "@jaspero/web-components": "1.76.5",
    "deepcopy": "2.1.0",
    "firebase": "10.7.1",
    "luxon": "3.4.4",
    "svelte-material-icons": "3.0.5"
  }
}
